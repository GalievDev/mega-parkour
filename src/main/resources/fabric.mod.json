{
  "schemaVersion": 1,
  "id": "${modId}",
  "version": "${modVersion}",
  "name": "${modName}",
  "description": "${modDescription}",
  "authors": [
    "Galiev"
  ],
  "contact": {
    "homepage": "https://galievdev.github.io",
    "issues": "https://github.com/galievdev/${modId}/issues",
    "sources": "https://github.com/galievdev/${modId}"
  },
  "license": "MIT Licence",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "net.galiev.megaparkour.MegaParkour"
      }
    ]
  },
  "custom": {
    "modmenu": {
      "badges": [],
      "update_checker": true
    },
    "mcb": [
      {
        "name": "Galiev Dev",
        "fillColor": -1275044945,
        "outlineColor": -100639825
      }
    ]
  },
  "mixins": [
    "megaparkour.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "fabricloader": ">=${loaderVersion}",
    "fabric-language-kotlin": ">=${fabricKotlinVersion}",
    "minecraft": ">=${minecraftVersion}",
    "java": ">=${javaVersion}"
  },
  "recommends": {
    "modmenu-badges-lib": "*"
  }
}